<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>cx_tf2_pose_tracker_plugin</name>
  <version>0.1.0</version>
  <description>Track poses via periodic tf lookups.</description>

  <maintainer email="viehmann@kbsg.rwth-aachen.de">Tarik Viehmann</maintainer>

  <license>Apache License 2.0</license>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <depend>rclcpp</depend>
  <depend>pluginlib</depend>
  <depend>cx_plugin</depend>
  <depend>cx_utils</depend>
  <depend>clips_vendor</depend>
  <depend>tf2_ros</depend>
  <depend>geometry_msgs</depend>

  <export>
    <build_type>ament_cmake</build_type>
    <cx_plugin plugin="${prefix}/tf2_pose_tracker_plugin.xml"/>
  </export>
</package>
